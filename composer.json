{
    "name": "sycms/resource-column-browser-bundle",
    "description": "Resource browser that uses columns.",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "require": {
        "symfony/framework-bundle": "^2.8",
        "symfony-cmf/resource-bundle": "^1.0@dev",
        "puli/repository": "@beta",
        "twig/twig": "^1.24",
        "symfony/twig-bundle": "^2.8"
    },
    "require-dev": {
        "symfony/console": "@stable",
        "symfony/yaml": "@stable",
        "symfony/process": "@stable",
        "phpunit/phpunit": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Sycms\\Bundle\\ColumnBrowserBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sycms\\Bundle\\ColumnBrowserBundle\\Tests\\": "tests/",
            "Sycms\\Bundle\\ColumnBrowserBundle\\Example\\": "example/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
